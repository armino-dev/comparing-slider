    "eslint": "^8.53.0",
    "eslint-config-jquery": "^3.0.2",
    "eslint-plugin-jquery": "^1.5.1",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.33",
